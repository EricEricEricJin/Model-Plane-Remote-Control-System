        PROC RUN()

        ENDPROC

        PROC SERVICE()
            IF STATUS[ALT] == TRUE
                IF ABS(TARGET[ALT] - DATA_LIST[ALT]) > ALT_LIMIT
                    // VS use absolute value
                ENDIF
            ENDIF
        ENDPROC

        PROC CHANGE_STATUS()

        ENDPROC
ENDCLASS